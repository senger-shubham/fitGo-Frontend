<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <!-- Allow cleartext (HTTP) traffic to specific domain (e.g., 192.168.29.95) -->
    <domain-config cleartextTrafficPermitted="true">
        <domain includeSubdomains="true">192.168.29.95</domain>
    </domain-config>
</network-security-config>